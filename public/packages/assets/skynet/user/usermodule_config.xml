        version="1.0" 
        description="乐逗登录"   />  -->

   
    
    <userModule
        moduleId="03"
        flag="0x00000003"
        init="true"
        className="com.skynet.android.user.impl.QQUserPlugin"
        resourcePath="skynet/user/qq"
        withApk="true"
        iconNormal="dgc_icon_qq.png"
        iconOnClick="dgc_icon_qq.png"
        version="1.0" 
        description="QQ帐号登录"  />
    
     
     <userModule
        moduleId="02"
        snsType="1"        
        flag="0x00000002"
        init="true"
        className="com.skynet.android.user.impl.SinaUserPlugin"
        resourcePath="skynet/user/sina"
        withApk="false"
        iconNormal="dgc_icon_xinlang.png"
        iconOnClick="dgc_icon_xinlang.png"
        version="1.0"
        description="新浪微博帐号登录"  />
    
    <userModule
        moduleId="04"
        flag="0x00000004"
        init="true"
        className="com.skynet.android.user.impl.RenRenUserPlugin"
        resourcePath="skynet/user/renren"
        withApk="false"
        iconNormal="dgc_icon_renren.png"
        iconOnClick="dgc_icon_renren.png"
        version="1.0" 
        description="人人网帐号登录"  />

</usermodules>