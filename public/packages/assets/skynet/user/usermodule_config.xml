<?xml version="1.0" encoding="UTF-8"?>
<usermodules >

    <!-- 配制用户模块子插件设置-->
    <!-- moduleId 模块编号 唯一标识 [必须出现]-->
    <!-- flag 标识 用于过滤  [必须出现]-->
    <!-- init 是否加载--> 
    <!-- className 子插件的类名 [必须出现]-->
    <!-- resourcePath 子插件加载资源路径-->
    <!-- snsType 社交类型-->    
        
    <!-- withApk 是否使用第三方APK-->    
    <!-- iconNormal 在登录 显示的普通ICON  -->
    <!-- iconOnClick 在登录 显示的按下的ICON  -->
    <!-- version 版本号  -->
    <!-- description 描述 -->
    
